prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")):i.currentSlide>=i.slideCount-1&&!0===i.options.centerMode&&(i.$nextArrow.addClass("slick-disabled").attr("aria-disabled","true"),i.$prevArrow.removeClass("slick-disabled").attr("aria-disabled","false")))},e.prototype.updateDots=function(){var i=this;null!==i.$dots&&(i.$dots.find("li").removeClass("slick-active").end(),i.$dots.find("li").eq(Math.floor(i.currentSlide/i.options.slidesToScroll)).addClass("slick-active"))},e.prototype.visibility=function(){var i=this;i.options.autoplay&&(document[i.hidden]?i.interrupted=!0:i.interrupted=!1)},i.fn.slick=function(){var i,t,o=this,s=arguments[0],n=Array.prototype.slice.call(arguments,1),r=o.length;for(i=0;i<r;i++)if("object"==typeof s||void 0===s?o[i].slick=new e(o[i],s):t=o[i].slick[s].apply(o[i].slick,n),void 0!==t)return t;return o}});
;
jQuery( function() {
    //Foreach image carousel slider in the page
    jQuery('.hds-meet-the-slider').each(function() {
        //Create the slider
        jQuery(this).not('.slick-initialized').slick({
            infinite: true,
            dots: false,
            slidesToShow: 4,
            slidesToScroll: 1,
            autoplay: false,
            autoplaySpeed: 5000,
            //link the next and previous buttons to the carousel
            prevArrow: jQuery(this).closest('.hds-meet-the').find('.hds-carousel-arrow-prev'),
            nextArrow: jQuery(this).closest('.hds-meet-the').find('.hds-carousel-arrow-next'),
            responsive: [
                {
                  breakpoint: 1274,
                  settings: {
                    slidesToShow: 3,
                    slidesToScroll: 1,
                  },
                },
				{
					breakpoint: 1054,
					settings: {
					  slidesToShow: 2,
					  slidesToScroll: 1,
					},
				  },
                {
                  breakpoint: 800,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                  }
                },
            ],
        });
    });
});
;
'use strict';

jQuery( function() {
    //Add Tabbed Section Block jQuery
    jQuery( '.hds-tab-nav-item' ).on( 'click', function( event ) {
        var tab_id = jQuery( this ).attr('data-tab-id');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tab-nav-item" ).removeClass('usa-active');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tab-nav-item button" ).attr('aria-selected', 'false');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tabbed-section-tab" ).removeClass('usa-active');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tabbed-section-tab" ).attr('hidden', '');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tab-nav-item[data-tab-id="+tab_id+"]" ).addClass('usa-active');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tab-nav-item[data-tab-id="+tab_id+"] button" ).attr('aria-selected', 'true');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tabbed-section-tab[data-tab-id="+tab_id+"]" ).addClass('usa-active');
        jQuery(this).closest('.hds-tabbed-section').find( ".hds-tabbed-section-tab[data-tab-id="+tab_id+"]" ).removeAttr('hidden');
    });

    jQuery( '.hds-tab-nav-item' ).on( 'keydown', function( event ) {
        var currTab = event.currentTarget;
        var nextTab = jQuery(this).next('li')[0];
        var prevTab = jQuery(this).prev('li')[0];
        var rent = jQuery(this).parent();
        var tabs = rent.children( 'li' );
        var firstTab = tabs[0];
        var lastTab = tabs[tabs.length - 1];
        if (currTab == lastTab) {
            nextTab = firstTab;
        }
        if (currTab == firstTab) {
            prevTab = lastTab;
        }

        var flag = false;
        switch (event.key) {
            case 'ArrowLeft':
                jQuery(prevTab).find('button').focus();
                flag = true;
                break;
            case 'ArrowRight':
                jQuery(nextTab).find('button').focus();
                flag = true;
                break;
            case 'Home':
                jQuery(firstTab).find('button').focus();    
                flag = true;
                break;
            case 'End':
                jQuery(lastTab).find('button').focus();
                flag = true;
                break;
            default:
                break;
        }

        if (flag) {
            event.stopPropagation();
            event.preventDefault();
        }
    });
});;
document.addEventListener("DOMContentLoaded",(function(){document.querySelectorAll(".SmdBlockIframeEmbedBlock").forEach((e=>{const t=e.querySelector(".smd-block-wrapper-inner"),i=e.querySelector("iframe.smd-iframe-iframe"),n=e.querySelector(".smd-placeholder"),r=e.getAttribute("data-ratio"),d=e.getAttribute("data-manual-height");function l(){if(window.innerWidth>1024)if("auto"===r){const e=t.clientWidth;t.style.height=e*(9/16)+"px"}else t.style.height=`${d}px`;else t.style.height="80svh"}n&&i&&n.addEventListener("click",(function(){n.style.display="none",i.src=i.getAttribute("data-src"),i.style.display="block"})),window.addEventListener("resize",l),l()}))}));;
